        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <AnchorPane prefHeight="388.0" prefWidth="536.0">
         <children>
            <Label layoutX="73.0" layoutY="36.0" text="Entrada" />
            <Label layoutX="356.0" layoutY="36.0" text="Saida" />
            <Label layoutX="219.0" layoutY="131.0" text="Pilha" />
            <Button layoutX="65.0" layoutY="106.0" mnemonicParsing="false" onAction="#campoEntrada" text="Entrada" />
            <Button layoutX="348.0" layoutY="106.0" mnemonicParsing="false" onAction="#campoSaida" text="Saida" />
            <TextField fx:id="campoEntrada" layoutX="19.0" layoutY="67.0" />
            <TextField fx:id="campoSaida" layoutX="296.0" layoutY="67.0" />
            <TextArea fx:id="campoPilha" layoutX="139.0" layoutY="165.0" prefHeight="162.0" prefWidth="200.0" />
         </children>
      </AnchorPane>
   </children>
</VBox>

<VBox alignment="CENTER" prefHeight="396.0" prefWidth="504.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo2.HelloController">
